{
	"compilerOptions": {
		"outDir": "dist",
		"target": "ES2020",
		"module": "ES2020",
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"strict": true,
		"skipLibCheck": true,
		"jsx": "react-jsx",
		"jsxImportSource": "react"
	},
	"include": ["source/**/*"]
}